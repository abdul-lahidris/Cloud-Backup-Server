{
  "name": "jwt_auth_node_typeorm",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only --exit-child src/server.ts",
    "build": "tsc -p tsconfig.build.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate": "rm -rf build && npx yarn build &&npx yarn typeorm migration:generate ./src/migrations/mainCreate -d ./src/utils/data-source.ts",
    "db:push": "rm -rf build && npx yarn build &&npx yarn typeorm migration:run -d src/utils/data-source.ts",
    "db:push:test": "rm -rf build && npx yarn build &&npx yarn typeorm migration:run -d src/utils/test-data-source.ts",
    "importData": "npx ts-node-dev --transpile-only --exit-child  src/data/seeder.ts",
    "test": "ts-node -r tsconfig-paths/register ./spec"
  },
  "dependencies": {
    "@types/archiver": "^5.3.2",
    "@types/multer": "^1.4.7",
    "archiver": "^6.0.0",
    "aws-sdk": "^2.1442.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "command-line-args": "^5.2.1",
    "config": "^3.3.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "envalid": "^7.3.1",
    "express": "^4.18.1",
    "html-to-text": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.5",
    "pg": "^8.4.0",
    "pug": "^3.0.2",
    "redis": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/command-line-args": "^5.2.0",
    "@types/config": "^0.0.41",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/html-to-text": "^8.1.0",
    "@types/jasmine": "^4.3.5",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.10",
    "@types/nodemailer": "^6.4.4",
    "@types/pug": "^2.0.6",
    "@types/supertest": "^2.0.12",
    "find": "^0.3.0",
    "jasmine": "^5.1.0",
    "jest": "^29.6.4",
    "morgan": "^1.10.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
